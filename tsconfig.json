  "compilerOptions": {
    "outDir": "./client/static/javascript/bundle",
    "module": "es2022",
    "target": "es5",
    "allowJs": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
  }
